---
import Section from "./Section.astro";
const { headline, introText, lenderProfiles, subheadline } = Astro.props;
---

<Section>
  <h1>{headline}</h1>
  <h2 class="!text-xl">{subheadline}</h2>
  {introText}
  {
    lenderProfiles.map((profile) => {
      return (
        <div>
          <h3>{profile.name}</h3>;
        </div>
      );
    })
  }
</Section>
